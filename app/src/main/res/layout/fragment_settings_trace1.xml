<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:title="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background1"
    android:orientation="vertical">

    <com.data.collection.view.TitleView
        android:id="@+id/title_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        title:IconLeft="@mipmap/top_back"
        title:TitleText="我的轨迹" />

    <LinearLayout
        android:id="@+id/datetime_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/background5"
        android:orientation="vertical"
        android:padding="@dimen/item_padding">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="选择时间："
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/starttime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="2019-5-27"
            android:textColor="@color/white"
            android:textSize="@dimen/font2" />
    </LinearLayout>
    <!-- open map -->
    <FrameLayout
        android:id="@+id/map_framelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>
